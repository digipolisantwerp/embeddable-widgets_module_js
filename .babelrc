{
  "env": {
    "test": {
      "plugins": ["istanbul"]
    }
  },
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "modules": "commonjs",
        "corejs": 2
      }
    ]
  ],
  "plugins": [
    "babel-plugin-add-module-exports",
    "@babel/plugin-transform-object-assign",
    "@babel/plugin-transform-react-jsx"
  ]
}
